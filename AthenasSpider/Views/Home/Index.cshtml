@{
    ViewBag.Title = "Home Page";
}

<link href="https://fonts.googleapis.com/css?family=Libre+Baskerville" rel="stylesheet">
<div class="jumbotron">
    
    <h1 class="title">Athena's Spider</h1>
    <p class="lead libre">A custom embroidery shop.</p> 
     @Html.ActionLink("Latest Works", "Index", "Product", null, new { @class = "btn btn-outline-light libre" })
</div>
<div id="carouselExampleSlidesOnly" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner" role="listbox">
        <div class="carousel-item active">
            <img class="d-block img-fluid" src="~/Content/Images/coverimg1.png" alt="First slide">
        </div>
        <div class="carousel-item">
            <img class="d-block img-fluid" src="~/Content/Images/coverimg2.png" alt="Second slide">
        </div>
        <div class="carousel-item">
            <img class="d-block img-fluid" src="~/Content/Images/Coverimg3.png" alt="Third slide">
        </div>
    </div>
</div>